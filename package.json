{
  "name": "seguros",
  "version": "1.0.0",
  "description": "Contrato inteligente para gesti√≥n de seguros en Hyperledger Fabric",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive",
    "test:coverage": "nyc mocha --recursive",
    "test:watch": "mocha --recursive --watch",
    "test:report": "nyc report --reporter=html"
  },
  "keywords": [
    "hyperledger",
    "fabric",
    "chaincode",
    "insurance"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fabric-contract-api": "^2.5.8",
    "fabric-shim": "^2.5.8"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "node_modules/**/*.js",
      "coverage/**/*.js"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "lines": 85,
    "functions": 85,
    "branches": 80,
    "statements": 85
  }
}
